{
    "name": "ievtds/laravel-module",
    "description": "Module management package for Laravel",
    "keywords": [
        "laravel",
        "module",
        "rad",
        "ievtds"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Denis Duli√ßi",
            "email": "info@ievtds.com",
            "homepage": "https://ievtds.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "laravel/framework": "^9.0|^10.0",
        "orchestra/testbench": "^7.4|^8.0",
        "phpunit/phpunit": "^9.5|^10.0"
    },
    "autoload": {
        "psr-4": {
            "ievtds\\Module\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "ievtds\\Module\\Providers\\Laravel"
            ],
            "aliases": {
                "Module": "ievtds\\Module\\Facade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
