{
    "name": "ievtds/laravel-debugbar-collector",
    "description": "Laravel Debugbar collector package for ievtds",
    "keywords": [
        "ievtds",
        "laravel",
        "debugbar"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Denis DuliÃ§i",
            "email": "info@ievtds.com",
            "homepage": "https://ievtds.com",
            "role": "Developer"
        },
        {
            "name": "Sevan Nerse",
            "email": "sevan@ievtds.com",
            "homepage": "https://ievtds.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=8.0",
        "barryvdh/laravel-debugbar": ">=3.6"
    },
    "require-dev": {
        "orchestra/testbench": ">=7.0",
        "phpunit/phpunit": ">=9.0"
    },
    "autoload": {
        "psr-4": {
            "ievtds\\DebugbarCollector\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ievtds\\DebugbarCollector\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "ievtds\\DebugbarCollector\\Provider"
            ]
        }
    }
}
